/* model */

.model-container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 99;
  display: flex;
  justify-content: center;
  align-items: center;
}

.model {
  width: 600rpx;
  background-color: #fff;
}

.model-text {
  color: #888;
  min-height: 100rpx;
  font-size: 30rpx;
  line-height: 1.5em;
  padding: 24rpx 40rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.model-btns {
  display: flex;
  justify-content: center;
  border-top: 1rpx solid #ddd;
}

.model-btn::after {
  /* border: none; */
  border-radius: 0;
  border: none;
}

.close-btn::after {
  border-right: 1rpx solid #ccc;
}

.model-btn {
  flex: 1;
  border-radius: 0;
}

page {
  background-color: #35354a;
  color: #fff;
}

.home {
  overflow: hidden;
}

/* 设置点位置 */

.home-swiper .wx-swiper-dots-horizontal {
  width: 100%;
  display: flex;
  padding-right: 30rpx;
  justify-content: flex-end;
}

/* 设置点样式 */

.home-swiper .wx-swiper-dot {
  display: inline-block;
  width: 24rpx;
  height: 5rpx;
  border-radius: 2.5rpx;
  background: #fff;
}

/* 当前点点的样式 */

.home-swiper .wx-swiper-dot::before {
  display: inline-block;
  width: 24rpx;
  height: 5rpx;
  content: '';
  flex-grow: 1;
  background: #fff;
}

/* 当前点点的样式 */

.home-swiper .wx-swiper-dot-active::before {
  display: inline-block;
  width: 24rpx;
  height: 5rpx;
  content: '';
  flex-grow: 1;
  background: #fec924;
}

.home-swiper {
  height: 350rpx;
}

.home-swiper-img {
  width: 100%;
  height: 100%;
}

.home-icons {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-shrink: 0;
  flex-wrap: nowrap;
  overflow: auto;
  border-bottom: 20rpx solid #2c2c3e;
  position: relative;
}

/* 去除滚动条 */

.home-icons::-webkit-scrollbar {
  width: 0;
  height: 0;
}

.home-icons-cover {
  width: 100%;
