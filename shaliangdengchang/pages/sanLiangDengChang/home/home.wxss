  left: 50%;
  transform: translateX(-50%);
  padding-right: 400rpx;
  color: #000;
  font-weight: bold;
  font-size: 24rpx;
  z-index: 100;
}

.pic-sharebox{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  bottom: 315rpx;
  left: 50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 102;
}

.pic-share {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #f6efe3;
  border: none;
  z-index: 102;
  padding: 0;
}

.btn-pic-img {
  width: 90rpx;
  height: 90rpx;
  z-index: 102;
}

.close-pic-icon {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  bottom: 160rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 100;
}

.pic-share::before, .pic-share::after {
