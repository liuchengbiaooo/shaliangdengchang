}

.pic-img {
  width: 520rpx;
  height: 100rpx;
  position: absolute;
  bottom: 605rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 100;
}

.pic-name {
  position: absolute;
  bottom: 716rpx;
