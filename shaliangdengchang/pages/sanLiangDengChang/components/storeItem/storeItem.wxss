.stores{
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-shrink: 0;
  flex-wrap: wrap;
  line-height: 60rpx;
  font-size: 30rpx;
}
.store{
  width: 50%;
  padding: 12rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.store-img-box{
  height: 0;
  padding-bottom: 100%;
  overflow: hidden;
}
.store-img{
  width: 100%;
}
.store-title{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.store-price{
  position: relative;
  display: flex;
}
.store-price-use{
  color: #f39900;
}
.store-price-del{
  color: #666;
  text-decoration: line-through;
  margin-left: 24rpx;
}
.store-shopping-car{
  position: absolute;
  right: 12rpx;
  color: #64c03a;
  font-size: 50rpx !important;
}
.store-add-car{
  color: #2653a7;
}




