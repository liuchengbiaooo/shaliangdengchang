page {
    background-color: #35354a;
    color: #999;
}

.mine-top {
    width: 100%;
    color: #fff;
    position: relative;
}

.mine-top-mark {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    z-index: -2
}

.mine-top-mark-img {
    width: 100%;
    height: 100%;
}

.user-info {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    flex-shrink: 0;
    padding-top: 80rpx;
    position: relative;
}

.setting {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    background-color: #665e7d;
    display: flex;
    justify-content: center;
    align-items: center;
    position: absolute;
    right: 45rpx;
    top: 30rpx;
}

.user-photo {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    border: 10rpx solid #828291;
}

.user-img {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.user-name {
    line-height: 60rpx;
}

.wallet {
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.wallet-money {
    width: 50%;
    font-size: 40rpx;
}

.wallet-money-mini {
    font-size: 24rpx;
}

.wallet-packet {
    display: flex;
    align-items: center;
}

.wallet-icon {
    margin-left: 24rpx;
    width: 36rpx;
    height: 36rpx;
}

.mine-item {
    height: 100rpx;
    border-bottom: 1rpx solid #2c2c3e;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 24rpx;
}

.mine-item-contact {
    border-radius: 0;
    background-color: transparent;
    color: #9e9da4;
    font-size: 28rpx;
}

.mine-item-text {
    margin-left: 12rpx;
}

.copyright {
    width: 100%;
    position: absolute;
    bottom: 60rpx;
    display: flex;
    line-height: 40rpx;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #fff;
}

.copyright-nav {
    color: #0792d4;
}

