page {
    background-color: #35354a;
    color: #999;
}

.mine-item {
    height: 100rpx;
    border-bottom: 1rpx solid #2c2c3e;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 24rpx;
}

.mine-item-contact {
    border-radius: 0;
    background-color: transparent;
    color: #9e9da4;
    font-size: 28rpx;
}

.mine-item-text {
    margin-left: 12rpx;
}