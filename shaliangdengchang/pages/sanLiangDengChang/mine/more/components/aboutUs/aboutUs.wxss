page {
    background-color: #35354a;
    color: #999;
}

.mine-item {
    height: 100rpx;
    border: 1rpx solid #2c2c3e;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 0 24rpx;
    margin-top: 30rpx;
}

.mine-item-contact {
    border-radius: 0;
    background-color: transparent;
    color: #9e9da4;
    font-size: 28rpx;
}

.mine-item-text {
    margin-left: 12rpx;
}

.aboutUs_img {
    width: 100%;
}

.aboutUs_img>image {
    width: 200rpx;
    height: 200rpx;
    margin: 0 auto;
    display: flex;
    justify-content: center;
}

.aboutUs_introduce {
    font-size: 26rpx;
    box-sizing: border-box;
    padding: 0rpx 0rpx 30rpx 46rpx;
}

.aboutUs_top {
    margin-top: 30rpx;
}